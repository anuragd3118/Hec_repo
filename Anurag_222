void solve(Node* root, map<int, int>&m, int level){
    if(!root){
        return;
    }
    if(m.find(level)==m.end()){
        m[level] = root->data;
    }
    solve(root->left, m, level+1);
    solve(root->right, m, level+1);
}
vector<int> leftView(Node *root)
{
   map<int,int>m;
   solve(root, m, 0);
   vector<int> ans;
   for(auto i:m){
       ans.push_back(i.second);
   }
   return ans;
}
